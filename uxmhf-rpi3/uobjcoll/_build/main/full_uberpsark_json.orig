{

   	/* manifest header */
	"uberspark-manifest":{
		"manifest_node_types" : [ "uberspark-uobj" ],
		"uberspark_min_version" : "6.0.0",
		"uberspark_max_version" : "any"
	},


  	/* uobj declaration */
	"uberspark-uobj" : {
		"namespace" : "uberspark/uobjcoll/platform/rpi3/uxmhf/main",
		"platform" : "rpi3",
		"arch" : "armv8_32",
		"cpu" : "generic",

		/* sources */
		"sources" : {
		    "h-files": [ "include/arm8-32.h", "include/atags.h",
				 "include/bcm2837.h", "include/config.h",
				 "include/ctxtrace.h", "include/debug.h",
				 "include/dmaprot.h", "include/fdt.h",
				 "include/guestos.h", "include/mailbox.h",
				 "include/miniuart.h", "include/pa5encfs.h",
				 "include/pl011uart.h", "include/types.h",
				 "include/uart.h", "include/uhcalltest.h",
				 "include/utpmtest.h", "include/uhsign.h" ],
		    "c-files": [ "core/main.c", "core/ghcall.c",
				 "uapps/uapp-watchdog.c",
				 "uapps/uapp-pa5encfs.c",
				 "uapps/uapp-utpmtest.c",
				 "uapps/uapp-uhsign.c",
				 "uapps/uapp-uhcalltest.c",
				 "core/hyptimer.c", "uapps/ctxtrace.c",
				 "core/bcm2837.c", "core/secboot.c",
				 "core/intprot.c", "core/dmaprot.c",
				 "core/uapi_s2pgtbl.c", "core/s2pgtbl.c",
				 "core/hyppgtbl.c", "core/mmu.c",
				 "core/hypvtable.c", "common/atags.c",
				 "common/debug.c", "common/subr_prf.c",
				 "common/mailbox.c",
				 "common/pl011uart.c", "common/miniuart.c",
				 "libutpm/utpm-common.c", "libutpm/utpm.c" ],
		    "casm-files": [ ],
		    "asm-files": [ "uapps/uapp-watchdogsup.s",
				   "core/hypvtablestubs.s",
				   "common/arm8-32.s", "core/entry.s"  ]
		},
	
		/* public methods */
		"publicmethods" : {
			"main" : [
				"void",
				"(uint32_t r0, uint32_t id, struct atag *at, uint32_t cpuid)", 
				"4"
			]
		},

		/* public methods of other uobjs that this uobj invokes 
			within the same collection
		*/
		"intrauobjcoll-callees" : {
		},
	

		/* public methods of other uobjs that this uobj invokes 
			across other collection(s)
		*/
		"interuobjcoll-callees": {
		},
	
		/* legacy CoSS functions/code that this uobj invokes */
		"legacy-callees": [
		],
	    "uobjrtl": [
		{
		    "namespace": "uberspark/uobjrtl/crt"
		},
		{
		    "namespace": "uberspark/uobjrtl/crypto"
		}
	    ]	    

	}

}
